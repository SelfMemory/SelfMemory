# Copy this file to .env and fill in your actual values

# SelfMemory API Client Configuration
SELFMEMORY_API_HOST=https://localhost:8080

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/selfmemory

# Embedding Configuration
EMBEDDING_MODEL=allminilm-v6
EMBEDDING_PROVIDER=ollama

# OLLAMA Configuration
OLLAMA_HOST=http://localhost:11434

# Search Configuration
DEFAULT_SEARCH_LIMIT=10
SIMILARITY_THRESHOLD=0.7
DUPLICATE_THRESHOLD=0.9

# Temporal Configuration
DEFAULT_TIMEZONE=UTC

# Database Configuration
SELFMEMORY_DATABASE=selfmemory

# Security Configuration
# ENCRYPTION_KEY=your_32_byte_base64_encoded_key_here

# SelfMemory Server Environment Configuration
# Copy this to .env and fill in your values

# Server Configuration
SELFMEMORY_SERVER_HOST=0.0.0.0
SELFMEMORY_SERVER_PORT=8081

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=selfmemory_memories

# Ollama Configuration
OLLAMA_HOST=http://localhost:11434
EMBEDDING_MODEL=nomic-embed-text

# Logging Level
LOG_LEVEL=INFO

# Environment
APP_ENV=development
