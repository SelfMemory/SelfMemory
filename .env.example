# Copy this file to .env and fill in your actual values

# InMemory API Client Configuration
INMEMORY_API_HOST=https://localhost:8080

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/inmemory

# Embedding Configuration
EMBEDDING_MODEL=allminilm-v6
EMBEDDING_PROVIDER=ollama

# OLLAMA Configuration
OLLAMA_HOST=http://localhost:11434

# Search Configuration
DEFAULT_SEARCH_LIMIT=10
SIMILARITY_THRESHOLD=0.7
DUPLICATE_THRESHOLD=0.9

# Temporal Configuration
DEFAULT_TIMEZONE=UTC

# Database Configuration
INMEMORY_DATABASE=inmemory

# Security Configuration
# ENCRYPTION_KEY=your_32_byte_base64_encoded_key_here
