# InMemory Server Environment Configuration
# Copy this to .env and fill in your values

# MongoDB Configuration (shared with dashboard)
MONGODB_URI=mongodb://localhost:27017/inmemory

# Server Configuration
INMEMORY_SERVER_HOST=0.0.0.0
INMEMORY_SERVER_PORT=8081

# ChromaDB Configuration (embedded, usually no config needed)
CHROMA_DB_PATH=/tmp/inmemory/chromadb

# Optional: Embedding Configuration
EMBEDDING_PROVIDER=chroma
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Optional: External embedding providers
# OPENAI_API_KEY=your_openai_key_here
# OLLAMA_HOST=http://localhost:11434

# Logging Level
LOG_LEVEL=INFO
